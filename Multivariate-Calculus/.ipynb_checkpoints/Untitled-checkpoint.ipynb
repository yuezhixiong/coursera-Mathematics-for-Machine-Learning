   ],
   "source": [
    "x, y, z = symbols('x y z')\n",
    "\n",
    "f = Matrix([x*exp(y)*cos(z) + 5*x**2*sin(y)*exp(z)])\n",
    "\n",
    "v = Matrix([x, y, z])\n",
    "\n",
    "f.jacobian(v)"
